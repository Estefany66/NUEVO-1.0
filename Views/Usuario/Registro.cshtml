@model Usuario
<h1>Subir Imagen </h1>

<div asp-validation-sumary="All"></div>
<form method="POST">
    <div class="form-group">
        <label asp-for="Nombre"></label>
        <input asp-for="Nombre" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="NombreFoto"></label>
        <input asp-for="NombreFoto" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Foto"></label>
        <input asp-for="Foto" class="form-control">
    </div>
    <button class="btn btn-primary">Subir Foto</button>
</form>
